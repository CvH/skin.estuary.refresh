						<param name="left" value="-640" />
						<param name="top" value="430" />
						<param name="width" value="2000" />
						<param name="flipx" value="true" />
					</include>
		<control type="fixedlist" id="$PARAM[list_id]">
			<left>-140</left>
			<width>list_width</width>
			<top>470</top>
			<height>550</height>
			<movement>5</movement>
			<focusposition>2</focusposition>
			<scrolltime tween="cubic" easing="out">500</scrolltime>
			<orientation>vertical</orientation>
			<pagecontrol>$PARAM[list_id]600</pagecontrol>
			<onleft>80600</onleft>
			<onright>012</onright>
			<onup>012</onup>
			<ondown>$PARAM[list_id]</ondown>
			<viewtype label="$PARAM[viewtype_label]">list</viewtype>
			<visible>Container.Content(sets)</visible>
			<focusedlayout height="list_item_height" width="list_width">
				<control type="image">
					<left>0</left>
					<right>0</right>
					<top>0</top>
					<bottom>0</bottom>
					<texture colordiffuse="button_focus">lists/focus.png</texture>
					<visible>Control.HasFocus($PARAM[list_id])</visible>
				</control>
				<control type="label">
					<left>70</left>
					<right>70</right>
					<top>0</top>
					<bottom>0</bottom>
					<aligny>center</aligny>
					<scroll>true</scroll>
					<font>font27</font>
					<label>$INFO[ListItem.Label]</label>
					<shadowcolor>text_shadow</shadowcolor>
				</control>
				<control type="label">
					<top>0</top>
					<bottom>0</bottom>
					<left>100</left>
					<right>20</right>
					<align>right</align>
					<aligny>center</aligny>
					<font>font12</font>
